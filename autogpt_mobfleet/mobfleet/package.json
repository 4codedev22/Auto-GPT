    "scripts": {
        "build": "yarn webpack --config webpack.config.js",
        "clean": "rimraf dist dist.tgz",
        "build:prod": "yarn clean && yarn build && yarn copy-resources",
        "copy-resources": "ts-node scripts/copy-resources.ts",
        "ts-node": "ts-node",
        "format": "prettier --write \"src/**/*.ts\"",
        "sonar:scanner": "sonar-scanner",
        "start:debug": "ts-node-dev --inspect -- src/main.ts",
        "start:prod": "cross-env GENERATE_SWAGGER=true yarn build:prod && node dist/bundle.js",
        "start": "cross-env GENERATE_SWAGGER=true ts-node-dev -- src/main.ts",
        "lint": "eslint . --ignore-path .server.eslintignore --no-eslintrc --config .server.eslintrc.json --ext .js,.ts",
        "lint:fix": "yarn lint --fix",
        "test": "yarn lint:fix && yarn lint && jest",
        "test:cov": "jest --coverage",
        "test:watch": "jest --watch ",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --silent --force-exit --coverage --config ./e2e/jest.e2e.config.json",
        "test:e2e:watch": "jest --watch --silent --force-exit --coverage --config ./e2e/jest.e2e.config.json",
        "typeorm": " ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/orm.config.ts",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
        "nest": "nest",
        "install:dev": "yarn --silent --ignore-optional  --non-interactive --link-duplicates  install",
        "deploy:dev": "yarn --silent build:prod --dev &&  yarn  --cwd dist pack --filename ./dist.tgz",
        "deploy:stg": "yarn --silent build:prod --stg &&  yarn  --cwd dist pack --filename ./dist.tgz",
        "deploy:prod": "yarn --silent build:prod --prod &&  yarn  --cwd dist pack --filename ./dist.tgz",
        "deploy:local": "yarn clean && yarn build && yarn copy-resources --local && yarn --cwd dist pack --filename ./dist.tgz"
    },
    "jest": {
        "coverageDirectory": "coverage",
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "testPathIgnorePatterns": [
            "./default.ts"
        ],
        "setupFiles": [
            "./e2e/setup.test.js"
        ],
        "testEnvironment": "node",
        "testRegex": "(/test/.*|\\.(spec))\\.(ts)$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        }
    },
